{
  "multipart": [
    {
      "apply": {
        "model": "tcwm:block/stationpane_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "tcwm:block/stationpane_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "tcwm:block/stationpane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "tcwm:block/stationpane_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "tcwm:block/stationpane_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "tcwm:block/stationpane_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "tcwm:block/stationpane_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "tcwm:block/stationpane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "tcwm:block/stationpane_noside",
        "y": 270
      }
    }
  ]
}